@model IEnumerable<CoreMVC_slutuppgift.Models.ApplicationUser>

@{
    ViewData["Title"] = "User List";
}

<div class="container mt-5">
    <h1>User List</h1>

    <table class="table table-bordered">
        <thead class="thead-light">
            <tr>
                
                <th>Email</th>
                <th>First Name</th>
                <th>Last Name</th>
                <th>Birth Date</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var user in Model)
            {
                <tr>
                    
                    <td>@user.Email</td>
                    <td>@user.FirstName</td>
                    <td>@user.LastName</td>
                    <td>@user.BirthDate.ToShortDateString()</td>
                </tr>
            }
        </tbody>
    </table>

    <a href="/account/register" class="btn btn-primary">Add New User</a>
</div>
